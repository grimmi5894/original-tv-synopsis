html 
  body 
    h1= showdata.title 
    h2 Developed By 
      ul  
        for name in showdata.developedBy 
          li= name
    h2 Starring 
      ul 
        for actors in showdata.starring 
          li= actors
    h2 Seasons 
      ul
        a(href=`/seasons/${showdata.seasons.number}`)  